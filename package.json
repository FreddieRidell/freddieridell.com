{
	"name": "FreddieRidell.com",
	"private": true,
	"engines": {
		"node": "12.x"
	},
	"prettier": {
		"singleQuote": false,
		"tabWidth": 4,
		"useTabs": true,
		"trailingComma": "all",
		"overrides": [
			{
				"files": "*.md",
				"options": {
					"tabWidth": 2
				}
			}
		]
	},
	"dependencies": {
		"@emotion/core": "^10.0.22",
		"@emotion/styled": "^10.0.23",
		"@freddieridell/kebab-case": "0.0.2",
		"@freddieridell/little-bonsai-styles": "^0.5.1",
		"date-fns": "^2.6.0",
		"emotion-theming": "^10.0.19",
		"gatsby-plugin-emotion": "^4.1.13",
		"gatsby-plugin-layout": "^1.1.13",
		"gatsby-plugin-manifest": "^2.2.26",
		"gatsby-plugin-sitemap": "^2.2.19",
		"gatsby-remark-prismjs": "^3.3.20",
		"gatsby-transformer-remark": "^2.6.32",
		"lodash.debounce": "^4.0.8",
		"polished": "^3.4.2",
		"prismjs": "^1.17.1",
		"ramda": "^0.26.1",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-helmet": "^5.2.1"
	},
	"scripts": {
		"build": "gatsby build",
		"start": "gatsby develop -H 0.0.0.0",
		"format": "prettier --write '**/*.js'",
		"now-build": "npm run build"
	},
	"devDependencies": {
		"gatsby": "^2.17.7",
		"gatsby-plugin-react-helmet": "^3.1.13",
		"gatsby-source-filesystem": "^2.1.35",
		"prettier": "^1.18.2"
	}
}
